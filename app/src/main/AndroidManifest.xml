<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicstructreapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CountryActivity"
            android:label="@string/category_country" />
        <activity
            android:name=".ReggaeActivity"
            android:label="@string/category_reggae" />
        <activity
            android:name=".RnBActivity"
            android:label="@string/category_rnb" />
        <activity
            android:name=".GospelActivity"
            android:label="@string/category_gospel" />
    </application>

</manifest>